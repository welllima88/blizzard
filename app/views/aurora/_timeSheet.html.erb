<div id="timesheet-wrapper">
  <!-- Timesheet Wrapper -->
  <div class="row" id="timesheet-login">
  	<div class="col-sm-12 col-md-6 col-md-offset-3 col-lg-4 col-lg-offset-4">
  		<div class="right_box ">
  			<h2>Clock In/Out</h2>
  			<div class="right_box_padding">
  				<div class="row">
  					<div class="col-sm-12">
  						<div class="input-group">
  							<span class="input-group-addon">Username</span>
  							<input type="text" id="timeclockUsername" class="form-control">
  						</div>
  					</div>
  					<div class="col-sm-12 top-buffer bottom-buffer">
  						<div class="input-group">
  							<span class="input-group-addon">Password</span>
  							<input type="text" id="timeclockPassword" class="form-control">
  						</div>
  					</div>
  				</div>
  				<input type="button" value="Punch Timecard" onclick="punch_timesheet();" class="btn btn-success">
  			</div>
  		</div>
  	</div>
  </div>

  <!-- Timesheet Result -->
  <div class="row" id="timesheet-result">
  	<div class="col-sm-12 col-md-6 col-md-offset-3 col-lg-4 col-lg-offset-4">
  		<h2 id="timeclock-result-header"></h2>
  		<p id="timeclock-result-text"></p>
  	</div>
  </div>
</div>